//******  Includes  *******************************************


//******  Variables  ******************************************
int ledpin= 13;

//******  Setup  **********************************************
void setup() {
pinMode (ledpin, OUTPUT);
}

//******  Loop  ***********************************************
void loop() {
digitalWrite (ledpin, HIGH);
delay (1000);
digitalWrite (ledpin, LOW);
delay (1000);
}
